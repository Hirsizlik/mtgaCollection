{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Localisation",
    "definitions": {
        "Localisation": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "langkey": {
                    "type": "string"
                },
                "isoCode": {
                    "type": "string"
                },
                "keys": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Key"
                    }
                }
            }
        },
        "Key": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "raw": {
                    "type": "string"
                },
                "text": {
                    "type": "string"
                }
            }
        }
    }
}
